{% extends 'base.html.twig' %}

{% block title %}Nurses Plannig {% endblock %}

{% block body %}
{{ form_start(form) }}
    {# {{ form_widget(form) }} #}
 
<div class="container" >
       
 <div class="col-sm-8 center-block" >

               
            <div class="body" display="inline">
                        <div class="form-group">
                 {{ form_label(form.FirstName) }}
                {{ form_widget(form.FirstName, {'attr': {'class': 'form-control'}}) }}
                {{ form_help(form.FirstName) }}
                {{ form_errors(form.FirstName) }}
                        </div>
                        <div class="form-group">
                           {{ form_label(form.Group) }}
                {{ form_widget(form.Group, {'attr': {'class': 'form-control'}}) }}
                {{ form_help(form.Group) }}
                {{ form_errors(form.Group) }}
                        </div>
       
             </div>
                   <div class="footer">
                      <button type="submit" class="btn btn-primary" >{{ button_label|default('Save') }}</button>
                    </div>


    <table class="table table-striped table-hover" class="col-sm-8">
                    <thead>

                        <tr>
                         
                            <th>FirstName</th>
                         
                        </tr>
                    </thead>
                   <tbody>
                   {% for nurse in nurses %}
                   
                      <tr>
                            
                            <td>{{ nurse.strenghts}}</td>
                          {{ dump( nurse.strenghts)}}
                        
                        </tr>
                        {% else %}
        <li><em>no user found</em></li>


 


  {% endfor %}
  </tbody>                      
  </table>
  {{ form_end(form) }}

              </div>
 </div>
    

  {% endblock %}